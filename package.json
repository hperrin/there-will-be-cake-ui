{
  "name": "there-will-be-cake-ui",
  "theme": "ui",
  "version": "1.0.0",
  "description": "Do you want some cake?",
  "keywords": [
    "ui",
    "theme",
    "pink"
  ],
  "repository": "https://github.com/hperrin/there-will-be-cake-ui",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "colorScheme": {
      "title": "Color Scheme",
      "description": "What kind of cake do you like?",
      "type": "string",
      "default": "Strawberry Shortcake",
      "enum": [
        "Strawberry Shortcake",
        "Vanilla",
        "Chocolate",
        "Carrot",
        "Mint",
        "Red Velvet",
        "Birthday",
        "Grape I Guess"
      ],
      "order": 1
    },
    "font": {
      "title": "Font",
      "description": "What font would you like your icing message in?",
      "type": "string",
      "default": "No Change",
      "enum": [
        "No Change",
        "Arial",
        "Helvetica",
        "Times New Roman",
        "Times",
        "Courier New",
        "Verdana",
        "Georgia",
        "Palatino",
        "Garamond",
        "Bookman",
        "Comic Sans MS",
        "Trebuchet MS",
        "Impact"
      ],
      "order": 2
    }
  },
  "main": "lib/main"
}
